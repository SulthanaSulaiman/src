public class mysqlinjection {
    
    public String escapeQuotes(String string)
    {
       if(string.contains("'")) {
        string= string.replaceAll("'", "\\\\'");
        System.out.println("Quotes:"+string);
       }
      return string  ;
      
    }
    
}
