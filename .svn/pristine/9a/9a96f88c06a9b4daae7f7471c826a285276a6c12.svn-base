package pathfinder.functions.projects.chaptersold;

/**
 * This Value Object (VO) is used to set and get chapter details.
 * @author Thanu
 */
public class ChapterDetailsVO_1 {

    private String chapterId = "";
    private String chapterName = "";
    private String stage = "";
    private String createDate = "";
    private String shipDate = "";
    private String mssPages = "";
    private String projId = "";

    public ChapterDetailsVO_1() {
    }

    public String getProjectId() {
        return this.projId;
    }

    public void setProjectId(String projId) {
        this.projId = projId;
    }

    public String getChapterId() {
        return this.chapterId;
    }

    public void setChapterId(String chapterId) {
        this.chapterId = chapterId;
    }

    public String getChapterName() {
        return this.chapterName;
    }

    public void setChapterName(String chapterName) {
        this.chapterName = chapterName;
    }

    public String getStage() {
        return this.stage;
    }

    public void setStage(String stage) {
        this.stage = stage;
    }

    public String getCreateDate() {
        return this.createDate;
    }

    public void setCreateDate(String createDate) {
        this.createDate = createDate;
    }

    public String getShipDate() {
        return this.shipDate;
    }

    public void setShipDate(String shipDate) {
        this.shipDate = shipDate;
    }

    public String getMssPages() {
        return this.mssPages;
    }

    public void setMssPages(String mssPages) {
        this.mssPages = mssPages;
    }
}
